<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.ujstore">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="UncleJoy Store Partner"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SwipeTabActivity"
            android:label="UncleJoy Store Partner" />
        <activity
            android:name=".SignUp_Activity"
            android:label="SignUp"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".privacy_activity"
            android:parentActivityName=".SwipeTabActivity" />
        <activity
            android:name=".term_activity"
            android:parentActivityName=".SwipeTabActivity" />
        <activity
            android:name=".setting_activity"
            android:parentActivityName=".SwipeTabActivity" />
        <activity android:name=".profile_activity"
            android:parentActivityName=".SwipeTabActivity" />
        <activity android:name=".sharable_activity"
            android:parentActivityName=".SwipeTabActivity"/>
        <activity android:name=".howworks_activity"
            android:parentActivityName=".SwipeTabActivity">
        </activity>
    </application>

</manifest>
